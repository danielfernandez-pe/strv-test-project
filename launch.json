{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program with .env",
        "program": "${workspaceFolder}/app.js",
        "runtimeExecutable": "nodemon",
        "runtimeArgs": ["--env-file=.env"],
        "console": "integratedTerminal",
        "envFile": "${workspaceFolder}/.env"
      }
    ]
  }